{
  "name": "Clear Code",
  "version": "0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Clear Code"
  },
  "icons": {
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "options_page": "bower_components/angular-chrome-options/app/index.html",
  "web_accessible_resources": ["icons/icon19.png", "clear-code-frame.html"]
}
